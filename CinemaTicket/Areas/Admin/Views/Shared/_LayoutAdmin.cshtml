<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <title>Corona Admin</title>

    <link rel="stylesheet" href="~/Areas/Admin/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/assets/vendors/css/vendor.bundle.base.css">

    <link rel="stylesheet" href="~/Areas/Admin/assets/vendors/jvectormap/jquery-jvectormap.css">
    <link rel="stylesheet" href="~/Areas/Admin/assets/vendors/flag-icon-css/css/flag-icon.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/assets/vendors/owl-carousel-2/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/assets/vendors/owl-carousel-2/owl.theme.default.min.css">

    <link rel="stylesheet" href="~/Areas/Admin/assets/css/style.css">
    <link rel="shortcut icon" href="~/Areas/Admin/assets/img/logo_mini.png" />
    @*@Styles.Render("~/Areas/Admin/css")
        @Scripts.Render("~/bundles/modernizr")*@

</head>
<body>

    <div class="container-scroller">
        @Html.Partial("_PartialSideBar")
        <div class="container-fluid page-body-wrapper">
            @Html.Partial("_PartialNavBar")
            <div class="main-panel">
                <div class="content-wrapper">
                    @RenderBody()
                </div>
                <footer class="footer float-none float-sm-right d-block mt-1 mt-sm-0 text-right">
                    <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
                </footer>
            </div>
        </div>
    </div>




    <script src="~/Areas/Admin/assets/vendors/js/vendor.bundle.base.js"></script>

    <script src="~/Areas/Admin/assets/vendors/progressbar.js/progressbar.min.js"></script>
    <script src="~/Areas/Admin/assets/vendors/jvectormap/jquery-jvectormap.min.js"></script>
    <script src="~/Areas/Admin/assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/Areas/Admin/assets/vendors/owl-carousel-2/owl.carousel.min.js"></script>

    <script src="~/Areas/Admin/assets/js/off-canvas.js"></script>
    <script src="~/Areas/Admin/assets/js/hoverable-collapse.js"></script>
    <script src="~/Areas/Admin/assets/js/misc.js"></script>
    <script src="~/Areas/Admin/assets/js/settings.js"></script>
    <script src="~/Areas/Admin/assets/js/todolist.js"></script>

    <script src="~/Scripts/plugins/dataTables/jquery.dataTables.js"></script>
    <script src="~/Scripts/plugins/dataTables/dataTables.fixedHeader.min.js"></script>
    <script src="~/Scripts/plugins/dataTables/dataTables.fixedColumns.min.js"></script>
    <script src="~/Scripts/plugins/dataTables/html5-button/dataTables.buttons.min.js"></script>
    <script src="~/Scripts/plugins/dataTables/html5-button/buttons.print.min.js"></script>
    <script src="~/Scripts/plugins/dataTables/html5-button/buttons.html5.min.js"></script>

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
    @RenderSection("upload", required: false)
</body>
</html>
